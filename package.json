{
    "bin": {
        "tweenpics-proxy": "index.js"
    },
    "dependencies": {
        "commander": "2.19.0",
        "@tweenpics/proxy-internal": "0.4.0"
    },
    "devDependencies": {
        "eslint": "5.8.0",
        "eslint-config-creative-area": "0.8.0"
    },
    "description": "a command-line tool to work with TweenPics locally",
    "engines": {
        "node": ">=8"
    },
    "eslintConfig": {
        "extends": "creative-area/es8",
        "env": {
            "node": true
        },
        "rules": {
            "no-console": "off",
            "no-magic-numbers": "off",
            "no-process-exit": "off",
            "no-param-reassign": [
                "error",
                {
                    "props": true,
                    "ignorePropertyModificationsFor": [
                        "response",
                        "responseHeaders",
                        "self"
                    ]
                }
            ]
        }
    },
    "license": "MIT",
    "name": "@tweenpics/proxy",
    "repository": "tweenpics/proxy",
    "version": "0.4.0"
}
