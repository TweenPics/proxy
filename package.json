{
    "bin": {
        "tweenpics-proxy": "index.js"
    },
    "dependencies": {
        "commander": "2.11.0",
        "fs-extra": "4.0.1",
        "http-proxy": "1.16.2",
        "karma-chrome-launcher": "2.2.0",
        "karma-firefox-launcher": "1.0.1",
        "request": "2.81.0",
        "sharp": "0.18.2",
        "wires": "1.1.1"
    },
    "devDependencies": {
        "eslint": "4.6.1",
        "eslint-config-creative-area": "0.7.0"
    },
    "description": "Proxy server to work with TweenPics locally",
    "engines": {
        "node": ">=6"
    },
    "eslintConfig": {
        "extends": "creative-area/es6",
        "env": {
            "node": true
        },
        "rules": {
            "global-require": "off",
            "no-console": "off",
            "no-magic-numbers": "off",
            "no-process-exit": "off",
            "no-sync": "off",
            "no-param-reassign": [
                "warn",
                {
                    "props": true,
                    "ignorePropertyModificationsFor": [ "headers", "response" ]
                }
            ]
        }
    },
    "license": "MIT",
    "name": "@tweenpics/proxy",
    "repository": "tweenpics/proxy",
    "version": "0.2.0-pre"
}
